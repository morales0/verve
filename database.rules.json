{
  
  "rules": {
    
    "users": {
      ".read": true,
      // Only users can access their personal data
      "$uid": {
        ".read": "auth != null && auth.uid == $uid",
        ".write": "$uid === auth.uid"
      }
    },
    "workouts": {
      // Only users can access their workout data
      "$uid": {
        ".read": "auth != null && auth.uid == $uid",
        ".write": "$uid === auth.uid"
      }
    },
  }
}